<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="cn.itcast.erp.entity.Store" table="store" >
        <id name="uuid" >
            <generator class="org.hibernate.id.enhanced.SequenceStyleGenerator"> <param name="sequence_name">store_seq</param> </generator>
        </id>

        <property name="name" /> 
        <property name="empuuid" /> 
 
    </class>
</hibernate-mapping>
