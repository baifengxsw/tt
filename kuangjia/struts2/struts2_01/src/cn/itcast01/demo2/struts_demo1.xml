<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>

	<!-- struts2为了管理Action的配置，通过包进行管理 -->
	<!-- 配置struts2的包 包名随便 但不可以重复 -->
	<package name="demo2" extends="struts-default" namespace="/">
		<!-- 配置action  name 是你访问的路径-->
		<action name="hello1" class="cn.itcast01.demo2.Actiondemo1">
		
		</action>
		<!-- 配置action  name 是你访问的路径-->
		<action name="hello2" class="cn.itcast01.demo2.Actiondemo2">
		
		</action>
		<action name="hello3" class="cn.itcast01.demo2.Actiondemo3"> </action>
	</package>
</struts>